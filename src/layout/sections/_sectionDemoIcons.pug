include ../dataset/_icons
.container.icon-wall
	.columns
		each item, index in icShowcase
			if item.name != 'badge-designed-with'
				.column.modal-button(id='ic-' + item.name data-slug=item.name data-target='icon-preview')
					figure
						span(class=prefix + 'ic ' + prefix + 'ic-' + item.name)
						figcaption
							| #{item.name}
						figcaption.unicode #{item.char}
